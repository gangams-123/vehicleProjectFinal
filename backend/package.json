{
  "name": "vehicle-backend",
  "version": "1.0.0",
  "description": "Vehicle showroom backend using Node.js, TypeScript, and MySQL",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec \"node --loader ts-node/esm src/server.ts\"",
    "build": "tsc",
    "start": "node dist/server.js"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "express",
    "mysql",
    "api",
    "vehicle"
  ],
  "author": "Ganga",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "multer": "^2.0.2",
    "mysql2": "^3.14.3",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
